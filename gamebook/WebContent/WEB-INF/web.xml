<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>gamebook</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Servlet Iniciar Sesion</servlet-name>
		<servlet-class>com.gamebook.controllers.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegistrarColaboracion</servlet-name>
		<servlet-class>com.gamebook.controllers.RegistrarColaboracion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AltaPropuesta</servlet-name>
		<servlet-class>com.gamebook.controllers.AltaPropuesta</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet Cargar Datos</servlet-name>
		<servlet-class>com.gamebook.controllers.CargarDatos</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AltaUsuario</servlet-name>
		<servlet-class>com.gamebook.controllers.AltaUsuario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NuevaColaboracion</servlet-name>
		<servlet-class>com.gamebook.controllers.NuevaColaboracion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConsultaUsuario</servlet-name>
		<servlet-class>com.gamebook.controllers.ConsultaUsuario</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConsultaPropuesta</servlet-name>
		<servlet-class>com.gamebook.controllers.ConsultaPropuesta</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>com.gamebook.controllers.Home</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Perfil</servlet-name>
		<servlet-class>com.gamebook.controllers.Perfil</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Usuarios</servlet-name>
		<servlet-class>com.gamebook.controllers.Usuarios</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Propuestas</servlet-name>
		<servlet-class>com.gamebook.controllers.Propuestas</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CerrarSesion</servlet-name>
		<servlet-class>com.gamebook.controllers.CerrarSesion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Servlet Iniciar Sesion</servlet-name>
		<url-pattern>/iniciar-sesion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ConsultaUsuario</servlet-name>
		<url-pattern>/consultaUsuario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ConsultaPropuesta</servlet-name>
		<url-pattern>/consultaPropuesta</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/WEB-INF/errorPages/404.jsp</location>
	</error-page>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Perfil</servlet-name>
		<url-pattern>/perfil</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Usuarios</servlet-name>
		<url-pattern>/usuarios</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Propuestas</servlet-name>
		<url-pattern>/propuestas</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AltaUsuario</servlet-name>
		<url-pattern>/altaUsuario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet Cargar Datos</servlet-name>
		<url-pattern>/cargarDatos</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AltaPropuesta</servlet-name>
		<url-pattern>/altaPropuesta</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CerrarSesion</servlet-name>
		<url-pattern>/cerrar</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NuevaColaboracion</servlet-name>
		<url-pattern>/nuevaColaboracion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegistrarColaboracion</servlet-name>
		<url-pattern>/registrarColaboracion</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>