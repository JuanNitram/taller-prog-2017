<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>MobileDevice</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>ControlAccesoFiltro</filter-name>
		<filter-class>com.culturarte.filtros.ControlAccesoFiltro</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ControlAccesoFiltro</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.culturarte.controllers.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet Cargar Datos</servlet-name>
		<servlet-class>com.culturarte.controllers.CargarDatos</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConsultaPropuesta</servlet-name>
		<servlet-class>com.culturarte.controllers.ConsultaPropuesta</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>com.culturarte.controllers.Home</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Propuestas</servlet-name>
		<servlet-class>com.culturarte.controllers.Propuestas</servlet-class>
	</servlet>
	<error-page>
		<error-code>404</error-code>
		<location>/WEB-INF/errorPages/404.jsp</location>
	</error-page>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ConsultaPropuesta</servlet-name>
		<url-pattern>/consultaPropuesta</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Propuestas</servlet-name>
		<url-pattern>/propuestas</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet Cargar Datos</servlet-name>
		<url-pattern>/cargarDatos</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>